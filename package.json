{
  "name": "EuTambem",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "lint": "eslint --ext .js .",
    "test": "jest --runInBand --verbose",
    "jest:update-snapshots": "jest --updateSnapshot",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/EuTambem/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.1",
    "react": "16.5.0",
    "react-native": "0.57.0",
    "react-native-button": "^2.3.0",
    "react-native-check-box": "^2.1.0",
    "react-native-datepicker": "^1.6.0",
    "react-native-dotenv": "^0.2.0",
    "react-native-modal": "^5.4.0",
    "react-native-search-box": "^0.0.19",
    "react-native-vector-icons": "^4.6.0",
    "react-navigation": "^2.17.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-react-native": "^3.2.1",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "^0.45.0",
    "npm": "^6.4.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.3.0",
    "react-test-renderer": "16.2.0",
    "sinon": "^4.5.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "setupFiles": [
      "./tests/jestSetup.js"
    ],
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-navigation)/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
